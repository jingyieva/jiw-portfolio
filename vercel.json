{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|webp|svg|ico|woff2)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ],
  "redirects": [
    { "source": "https://www.jiw-portfolio.vercel.app/(.*)", "destination": "https://jiw-portfolio.vercel.app/$1", "permanent": true }
  ]
}
